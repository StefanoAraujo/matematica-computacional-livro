[latexmath]
++++
\[
\begin{array}{rcrcrcrcr}
\ell_1:&& x_1 &+&  x_2 &-&  x_3 & =& 0\\
\ell_2:&&2x_1 &+& 3x_2 &+&  x_3 & =& 0\\
\ell_3:&& x_1 &+& 4x_2 &+& 2x_3 & =& 0
